URxvt.foreground: #F1FFDA
URxvt.cursorColor: #344E41

! black
URxvt.color0: #AF7A5F
URxvt.color8: #585B70

! red
URxvt.color1: #F38BA8
URxvt.color9: #F38BA8

! green
